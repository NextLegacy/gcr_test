{
    "tasks": {
        "dev": "deno run -A npm:vite dev",
        "build": "deno run -A npm:vite build",
        "preview": "deno run -A npm:vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "machine-translate": "inlang machine translate --project project.inlang"
    },
    "imports": {
        "@eslint/compat": "npm:@eslint/compat@^1.2.7",
        "@eslint/js": "npm:@eslint/js@^9.22.0",
        "@fontsource/fira-mono": "npm:@fontsource/fira-mono@^5.2.5",
        "@neoconfetti/svelte": "npm:@neoconfetti/svelte@^2.2.2",
        "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
        "@sveltejs/adapter-node": "npm:@sveltejs/adapter-node@^5.2.12",
        "@sveltejs/kit": "npm:@sveltejs/kit@^2.20.1",
        "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.3",
        "@tailwindcss/forms": "npm:@tailwindcss/forms@^0.5.10",
        "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.16",
        "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.14",
        "drizzle-kit": "npm:drizzle-kit@^0.30.5",
        "eslint": "npm:eslint@^9.22.0",
        "eslint-config-prettier": "npm:eslint-config-prettier@^10.1.1",
        "eslint-plugin-svelte": "npm:eslint-plugin-svelte@^3.3.3",
        "globals": "npm:globals@^16.0.0",
        "mdsvex": "npm:mdsvex@^0.12.3",
        "prettier": "npm:prettier@^3.5.3",
        "prettier-plugin-svelte": "npm:prettier-plugin-svelte@^3.3.3",
        "prettier-plugin-tailwindcss": "npm:prettier-plugin-tailwindcss@^0.6.11",
        "svelte": "npm:svelte@^5.23.2",
        "tailwindcss": "npm:tailwindcss@^4.0.14",
        "typescript": "npm:typescript@^5.8.2",
        "typescript-eslint": "npm:typescript-eslint@^8.27.0",
        "vite": "npm:vite@^6.2.2",
        "@inlang/paraglide-js": "npm:@inlang/paraglide-js@^2.0.1",
        "@inlang/cli": "npm:@inlang/cli@^3.0.5",
        "@fontsource-variable/comfortaa": "npm:@fontsource-variable/comfortaa@^5.2.5",
        "@fontsource-variable/fira-code": "npm:@fontsource-variable/fira-code@^5.2.5",
        "@fontsource-variable/maven-pro": "npm:@fontsource-variable/maven-pro@^5.2.6",
        "@iconify/json": "npm:@iconify/json@^2.2.318",
        "@img/sharp-wasm32": "npm:@img/sharp-wasm32@^0.33.5",
        "@neondatabase/serverless": "npm:@neondatabase/serverless@^0.10.4",
        "argon2": "npm:argon2@^0.41.1",
        "@oslojs/crypto": "npm:@oslojs/crypto@^1.0.1",
        "@oslojs/encoding": "npm:@oslojs/encoding@^1.1.0",
        "@sveltejs/vite-plugin-svelte-inspector": "npm:@sveltejs/vite-plugin-svelte-inspector@^4.0.1",
        "drizzle-orm": "npm:drizzle-orm@^0.40.1",
        "sharp": "npm:sharp@^0.33.5",
        "tailwind-scrollbar": "npm:tailwind-scrollbar@^4.0.1",
        "unplugin-icons": "npm:unplugin-icons@^22.1.0",
        "vite-imagetools": "npm:vite-imagetools@^7.0.5",
        "@types/jsonld": "npm:@types/jsonld@^1.5.15",
        "vite-plugin-kit-routes": "npm:vite-plugin-kit-routes@^0.8.4",
        "@fontsource-variable/roboto-slab": "npm:@fontsource-variable/roboto-slab@^5.2.5",
        "@threlte/core": "npm:@threlte/core@^8.0.1",
        "motion": "npm:motion@^12.5.0",
        "@threlte/rapier": "npm:@threlte/rapier@^3.1.0",
        "minisearch": "npm:minisearch@^7.1.2",
        "three": "npm:three@^0.174.0",
        "rollup-plugin-visualizer": "npm:rollup-plugin-visualizer@^5.14.0",
